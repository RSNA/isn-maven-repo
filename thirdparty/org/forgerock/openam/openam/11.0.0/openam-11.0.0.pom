    <!-- Component Properties -->
    <properties>
        <!-- **************************************************************** -->
        <maven.build.timestamp.format>yyyy-MMM-dd HH:mm:ss</maven.build.timestamp.format>
        <!-- **************************************************************** -->

        <!-- Java Properties -->
        <java.source.version>1.5</java.source.version>
        <java.target.version>1.5</java.target.version>
        <federation.java.source.version>1.6</federation.java.source.version>
        <federation.java.target.version>1.6</federation.java.target.version>

        <!-- Surefire Argument Line -->
        <java.surefire.options>-Xms2g -Xmx2g -XX:MaxPermSize=512m</java.surefire.options>

        <!-- JavaDoc Setting Properties -->
        <javadoc.options>-J-Xms2g -J-Xmx2g -J-XX:MaxPermSize=512m</javadoc.options>
        <!-- Shade plugin setting for ClientSDK build -->
        <generate.clientsdk.sources>false</generate.clientsdk.sources>

        <!-- Project Encoding -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <!-- Version Properties -->
