        <forgerock.json.version>2.0.2</forgerock.json.version>
        <commons.authfilter.version>1.2.0</commons.authfilter.version>
        <forgerock-ui.version>1.0.1</forgerock-ui.version>
        <restlet.version>2.1.1</restlet.version>
        <xercesj.version>2.11.0</xercesj.version>
        <powermock.version>1.5</powermock.version>
        <jackson.version>2.1.2</jackson.version>

        <openam.version>OpenAM ${project.version}</openam.version>
        <!--  Project web site -->
        <siteDistributionURL>scp://forgerock.org/var/www/vhosts/openam.forgerock.org/httpdocs</siteDistributionURL>
    </properties>

    <!-- Profiles -->
    <profiles>
        <!-- Place Default Profiles here to override and determine Environment. -->

        <profile>
            <id>default-profile</id>
            <!-- This profile gets activated for WINDOWS and LINUX Systems -->
            <activation>
                <file>
                    <exists>${env.JAVA_HOME}/lib/tools.jar</exists>
                </file>
            </activation>
            <properties>
                <toolsjar>${env.JAVA_HOME}/lib/tools.jar</toolsjar>
            </properties>
            <dependencies>
                <dependency>
                    <groupId>com.sun</groupId>
                    <artifactId>tools</artifactId>
                    <version>${java.target.version}</version>
                    <scope>system</scope>
                    <optional>true</optional>
                    <systemPath>${toolsjar}</systemPath>
                </dependency>
            </dependencies>
        </profile>
        <profile>
            <id>release</id>
            <activation>
                <property>
                    <name>performRelease</name>
                    <value>true</value>
                </property>
            </activation>
            <properties>
                <generate.clientsdk.sources>true</generate.clientsdk.sources>
            </properties>
        </profile>
        <profile>
            <id>binary-licensing</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-scm-plugin</artifactId>
                        <inherited>false</inherited>
                        <executions>
                            <execution>
                                <id>Grab binary license</id>
                                <phase>validate</phase>
                                <goals>
                                    <goal>checkout</goal>
                                </goals>
                                <configuration>
                                    <connectionUrl>scm:svn:https://svn.forgerock.org/internal/trunk/QA/OpenDJ/builds/b2.6/license/</connectionUrl>
                                    <checkoutDirectory>${project.build.directory}/legal-notices</checkoutDirectory>
                                </configuration>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>

